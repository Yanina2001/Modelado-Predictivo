   "metadata": {},
   "outputs": [],
   "source": [
    "y_km_train_df = pd.DataFrame(y_km_train, columns = ['cluster']).set_index(indexes_train, drop=True, append=False, inplace=False, verify_integrity=False)"
   ]
  },
  {
   "cell_type": "code",
