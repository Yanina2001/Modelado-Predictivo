   "source": [
    "### Automatización de validación cruzada para cierto modelo"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 52,
   "metadata": {},
   "outputs": [],
   "source": [
    "def train_model(model, model_name, x_tr, y_tr, x_ts, y_ts, scoring):\n",
    "    print(model_name)\n",
    "    \n",
    "    # Fitting model\n",
    "    kfold = model_selection.KFold(n_splits=5)\n",
    "    cv_results = model_selection.cross_validate(model, x_tr, y_tr, cv=kfold, scoring=scoring)\n",
    "    #print(cv_results)\n",
    "    \n",
    "    for i in range(len(scoring)):\n",
    "        sc = scoring[i]\n",
    "        msg = \"Estimation of E_out (%s): %f (%f)\" % (sc, cv_results['test_' + sc].mean(), cv_results['test_' + sc].std())\n",
    "        print(msg)\n",
    "        print(cv_results['test_' + sc], '\\n')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
