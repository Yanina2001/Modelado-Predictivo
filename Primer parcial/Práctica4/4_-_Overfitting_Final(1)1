   "source": [
    "def check_nans(df):\n",
    "    for c in list(df.columns.values):\n",
    "        print(c + ' NaNs -> ' + str(df[c].isnull().sum()))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def plot_hist(column):\n",
    "    skewness = calc_skewness(column)\n",
    "    __fig__, __axs__ = plt.subplots(ncols=1, figsize=(15, 5))\n",
    "    __fig__.subplots_adjust(hspace=0.1, wspace=0.3)\n",
    "    \n",
    "    __axs__.set(title=\"Column (mean = {}, std = {}, skewness = {})\".format(round(column.mean(), 2), round(column.std(), 2), round(skewness, 2)))\n",
    "    _ = sns.histplot(x=column, ax=__axs__)\n",
    "    _.axvline(x=column.mean(),\n",
    "                color='blue',\n",
    "                ls='--', \n",
    "                lw=2.5)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "def calc_skewness(x):\n",
    "    return (((x - x.mean()) / (x.var() ** (1/2))) ** 3).mean()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "V1 NaNs -> 0\n",
      "V2 NaNs -> 0\n",
      "V3 NaNs -> 0\n",
      "V4 NaNs -> 0\n",
      "V5 NaNs -> 0\n",
      "V6 NaNs -> 0\n",
      "V7 NaNs -> 0\n"
     ]
    }
