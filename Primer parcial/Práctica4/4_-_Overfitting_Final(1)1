    }
   ],
   "source": [
    "model_lr = linear_model.LogisticRegression(penalty = 'l2', max_iter = 1000, verbose = True)\n",
    "model_name = 'Logistic Regression'\n",
    "scoring = ['accuracy', 'neg_log_loss']\n",
    "train_model(model_lr, model_name, X_train_model, y_train_model, X_test_model, y_test_model, scoring)"
   ]
  },
